{
  "name": "@gusbot/website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build-only": "pnpm run lint && next build",
    "data": "pnpm run --filter scraper data",
    "build-data": "pnpm run lint && pnpm run --filter scraper data && next build",
    "build": "pnpm run build-data",
    "build-and-push": "pnpm run build-data && git diff --quiet --exit-code packages/website/investigation/giveaway_leavers.json || (git add packages/website/investigation/giveaway_leavers.json && git commit -m 'chore: updated investigation file' && git push)",
    "start": "next start",
    "lint": "next lint",
    "serve": "npx serve out",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.2.7",
    "date-fns": "^4.1.0",
    "masonic": "^4.1.0",
    "next": "^15.5.7",
    "react": "^19.0.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.0.0",
    "react-virtuoso": "^4.14.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^9",
    "eslint-config-next": "^15.5.7",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
